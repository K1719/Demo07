<Page
    x:Class="MoneyCalculator.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MoneyCalculator"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <FlyoutBase.AttachedFlyout>
            <TimePickerFlyout/>
        </FlyoutBase.AttachedFlyout>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Margin="219,48,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox1" HorizontalAlignment="Left" Margin="54,48,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top"/>
        <TextBlock x:Name="eurotTextBlock" HorizontalAlignment="Left" Margin="54,80,0,0" TextWrapping="Wrap" Text="Eurot" VerticalAlignment="Top"/>
        <TextBlock x:Name="markatTextBlock" HorizontalAlignment="Left" Margin="219,80,0,0" TextWrapping="Wrap" Text="Markat" VerticalAlignment="Top"/>

    </Grid>
</Page>
